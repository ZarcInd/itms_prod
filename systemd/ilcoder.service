[Unit]
Description=Service that keeps running the php script for accepting raw tcp request
After=network.target mysql.service

[Install]
WantedBy=multi-user.target

[Service]
Type=Simple
ExecStart=/usr/sbin/ilcoder.sh
WorkingDir=/var/www
Restart=on-failure
RestartSec=1
StartLimitBurst=5
StartLimitInterval=0
StandardOutput=journal
StandardError=journal
SyslogIdentifier=ilcoder